<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_dlc_jolly/vehicles/lxa_vehicle_truck_vlad/lxa_vehicle_truck_vlad" orientation_object="rp_police_truck" />
	<sequence_manager file="units/pd2_dlc_jolly/vehicles/lxa_vehicle_truck_vlad/lxa_vehicle_truck_vlad" />
	<animations>
		<animation_group name="ag_right_door">
			<object name="a_right_door" />
		</animation_group>

		<animation_group name="ag_left_door">
			<object name="a_left_door" />
		</animation_group>
	</animations>
	
	<bodies>
		<body name="body" enabled="true" template="animated">
			<object name="a_body"/>
			<object name="c_box_1" collision_type="box" padding="-2"/>
			<object name="c_box_2" collision_type="box" padding="-2"/>
			<object name="c_box_3" collision_type="box" padding="-2"/>
			<object name="c_box_4" collision_type="box" padding="-2"/>
			<object name="c_box_5" collision_type="box" padding="-2"/>
			<object name="c_box_6" collision_type="box" padding="-2"/>
			<object name="c_box_7" collision_type="box" padding="-2"/>
			<object name="c_box_8" collision_type="box" padding="-2"/>
			<object name="c_box_9" collision_type="box" padding="-2"/>
			<object name="c_box_10" collision_type="box" padding="-2"/>
			<object name="c_box_11" collision_type="box" padding="-2"/>
			<object name="c_box_12" collision_type="box" padding="-2"/>
			<object name="c_box_13" collision_type="box" padding="-2"/>
		</body>
		
		<body remove_ray_type="ai_vision" name="window_front" enabled="true" template="animated">
			<object name="c_window_front" collision_type="box" padding="-2"/>
		</body>
		<body remove_ray_type="ai_vision" name="window_right" enabled="true" template="animated">
			<object name="c_window_right" collision_type="box" padding="-2"/>
		</body>
		<body remove_ray_type="ai_vision" name="window_left" enabled="true" template="animated">
			<object name="c_window_left" collision_type="box" padding="-2"/>
		</body>
		<body remove_ray_type="ai_vision" name="window_right_door" enabled="true" template="animated">
			<object name="c_window_right_door" collision_type="box" padding="-2"/>
		</body>
		 <body remove_ray_type="ai_vision" name="window_left_door" enabled="true" template="animated">
			<object name="c_window_left_door" collision_type="box" padding="-2"/>
		</body>
		
		<body name="static_body2" enabled="true" template="animated">
			<object name="a_left_door" />
			<object name="c_box_left_door_1" collision_type="box" padding="-2"/>
			<object name="c_box_left_door_2" collision_type="box" padding="-2"/>
			<object name="c_box_left_door_3" collision_type="box" padding="-2"/>
			<object name="c_box_left_door_4" collision_type="box" padding="-2"/>
		</body>
		
		<body name="static_body3" enabled="true" template="animated">
			<object name="a_right_door" />
			<object name="c_box_right_door_1" collision_type="box" padding="-2"/>
			<object name="c_box_right_door_2" collision_type="box" padding="-2"/>
			<object name="c_box_right_door_3" collision_type="box" padding="-2"/>
			<object name="c_box_right_door_4" collision_type="box" padding="-2"/>
		</body>
	</bodies>
	
	<decal_surfaces default_material="metal_chassis">
		<decal_mesh name="dm_police_truck" 			enabled="true" material="metal"/>
		<decal_mesh name="dm_left_door" 			enabled="true" material="metal" />
		<decal_mesh name="dm_right_door" 			enabled="true" material="metal" />
		<decal_mesh name="dm_tier" 					enabled="true" material="rubber" />
		<decal_mesh name="dm_window_right_door" 	enabled="true" material="glass_unbreakable" />
		<decal_mesh name="dm_window_left_door" 		enabled="true" material="glass_unbreakable" />
		<decal_mesh name="dm_window_right" 			enabled="true" material="glass_unbreakable" />
		<decal_mesh name="dm_window_left" 			enabled="true" material="glass_unbreakable" />
		<decal_mesh name="dm_window_front" 			enabled="true" material="glass_unbreakable" />   
	</decal_surfaces>
	
	<graphics>
		<object name="g_undercarriage" 				enabled="true" />
		<object name="g_police_truck" 				enabled="true" />
		<object name="g_police_truck_interior" 		enabled="true" />
		<object name="g_step_01" 					enabled="true" />
		<object name="g_step_02" 					enabled="true" />
		<object name="g_rear_wheels" 				enabled="true" />
		<object name="g_front_left_wheel" 			enabled="true" />
		<object name="g_front_right_wheel" 			enabled="true" />
